{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "routeFinance": {
        "ClusterId": "clusterFinanceApi",
        "Match": {
          "Path": "/finances/{**remainder}"
        }
      },
      "routeRpcFinance": {
        "ClusterId": "clusterFinanceApi",
        "Match": {
          "Path": "/rpc/finances/{**remainder}"
        }
      },
      "routeDefault": {
        "ClusterId": "clusterDefault",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "clusterDefault": {
        "Destinations": {
          "destinationDefault": {
            "Address": "http://api-local-origin:8000/"
          }
        }
      },
      "clusterFinanceApi": {
        "Destinations": {
          "destinationFinance": {
            "Address": "http://localhost:5015/"
          }
        }
      }
    }

  }
}